%% nerd font begin
{% if cookiecutter.nerd_font != "n" -%}
\newfontface\nerdfont{
  {%- if cookiecutter.nerd_font|lower == "y" -%}
  CaskaydiaCove~Nerd~Font
  {%- else -%}
  {{ cookiecutter.nerd_font|replace(" ", "~")}}
  {%- endif -%}
}
{% raw -%}
\NewDocumentCommand{\nf}{m o}{{\IfNoValueF{#2}{\color{#2}}\nerdfont #1}}
{%- endraw %}
{%- endif %}
%% nerd font end
