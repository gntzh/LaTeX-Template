%% box begin
{% raw -%}
\NewTColorBox{block}{O{success} O{\MakeUppercase{#1}} O{}}{
  enhanced~jigsaw,
  breakable,
  sharp~corners,
  boxrule=0em,
  right=1em,
  left=1em,
  toptitle=0.8em,
  title=#2,
  fonttitle={\bfseries},
  coltitle=#1,
  colback=#1!5,
  colbacktitle=#1!5,
  borderline~west={0.5em}{0em}{#1},
  #3
}
{%- endraw %}
%% box end

